    108  9000 ????				      MAC	clean_start
    109  9000 ????				      sei
    110  9000 ????				      cld
    111  9000 ????
    112  9000 ????				      ldx	#0
    113  9000 ????				      txa
    114  9000 ????				      tay
    115  9000 ????			   .CLEAR_STACK dex
    116  9000 ????				      txs
    117  9000 ????				      pha
    118  9000 ????				      bne	.CLEAR_STACK	; SP=$FF, X = A = Y = 0
